<h1> cat index </h1>

<ul> 
    <% @cats.each do |cat| %>
        <li>
            <%= cat.name %>
            <%= cat.color %>
            <%= cat.sex %>
            <%= cat.birth_date %>
            <%= cat.description %>
        </li>
        <a href="<%= cat_path(cat.id)%>"> cat show page </a>
    <% end %>
</ul>